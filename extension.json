{
	"name": "Shibboleth",
	"version": "2.0",
	"author": [ "Christopher Odenbach" ],
	"url": "https://www.mediawiki.org/wiki/Extension:Shibboleth",
	"descriptionmsg": "shibboleth-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.38.0",
		"platform": {
			"php": ">=7.1"
		},
		"extensions": {
			"PluggableAuth": "7.*"
		}
	},
	"MessagesDirs": {
		"Shibboleth": [
			"i18n"
		]
	},
	"ConfigRegistry": {
		"Shibboleth": "GlobalVarConfig::newInstance"
	},
	"config": {
		"Shibboleth_Username": {
			"value": "eppn"
		},
		"Shibboleth_Email": {
			"value": "mail"
		},
		"Shibboleth_DisplayName": {
			"value": "displayName"
		},
		"Shibboleth_DisplayNameFormatString": {
			"value": null
		},
		"Shibboleth_GroupMap": {
			"value": null
		},
		"Shibboleth_GroupMap_attr_may_be_empty": {
			"value": false
		},
		"Shibboleth_Logout_Base_Url": {
			"value": null
		},
		"Shibboleth_Logout_Target_Url": {
			"value": null
		}
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\Shibboleth\\": "src/"
	},
	"Hooks": {},
	"callback": "MediaWiki\\Extension\\Shibboleth\\ShibbolethHooks::onRegistration",
	"attributes": {
		"PluggableAuth": {
			"Shibboleth": {
				"class": "MediaWiki\\Extension\\Shibboleth\\PluggableAuth",
				"services": [
					"UserFactory",
					"AuthManager",
					"UserGroupManager"
				]
			},
			"LoginSpecialPages": [ "Shibboleth" ]
		}
	},
	"SpecialPages": {
		"Shibboleth": {
			"class": "\\MediaWiki\\Extension\\Shibboleth\\SpecialShibboleth",
			"services": [
				"PluggableAuthFactory",
				"AuthManager",
				"PluggableAuth.GroupProcessorRunner"
			]
		}
	},
	"ExtensionMessagesFiles": {
		"ShibbolethAlias": "Shibboleth.alias.php"
	},
	"manifest_version": 2
}
